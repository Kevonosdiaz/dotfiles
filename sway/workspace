# Two digit workspace style for flexibility

# Switching to workspace [n][m]
bindsym $mod+1 mode ws1
bindsym $mod+2 mode ws2
bindsym $mod+3 mode ws3
bindsym $mod+4 mode ws4
bindsym $mod+5 mode ws5
bindsym $mod+6 mode ws6
bindsym $mod+7 mode ws7
bindsym $mod+8 mode ws8
bindsym $mod+9 mode ws9

# Moving current container to workspace [n][m]
bindsym $mod+Shift+1 mode mws1
bindsym $mod+Shift+2 mode mws2
bindsym $mod+Shift+3 mode mws3
bindsym $mod+Shift+4 mode mws4
bindsym $mod+Shift+5 mode mws5
bindsym $mod+Shift+6 mode mws6
bindsym $mod+Shift+7 mode mws7
bindsym $mod+Shift+8 mode mws8
bindsym $mod+Shift+9 mode mws9

#
# You shouldn't need to edit anything below here.
#



#
# Workspaces:
#
    # Switch to workspace
    bindsym $mod+1 workspace number 1
    bindsym $mod+2 workspace number 2
    bindsym $mod+3 workspace number 3
    bindsym $mod+4 workspace number 4
    bindsym $mod+5 workspace number 5

    # For other set on other monitor
    bindsym $mod+6 workspace A
    bindsym $mod+7 workspace B
    bindsym $mod+8 workspace C
    bindsym $mod+9 workspace D
    bindsym $mod+0 workspace E

    # Alternative bindings for comfort
    bindsym $mod+Ctrl+1 workspace A
    bindsym $mod+Ctrl+2 workspace B
    bindsym $mod+Ctrl+3 workspace C
    bindsym $mod+Ctrl+4 workspace D
    bindsym $mod+Ctrl+5 workspace E

    # Move focused container to workspace
    bindsym $mod+Shift+1 move container to workspace number 1
    bindsym $mod+Shift+2 move container to workspace number 2
    bindsym $mod+Shift+3 move container to workspace number 3
    bindsym $mod+Shift+4 move container to workspace number 4
    bindsym $mod+Shift+5 move container to workspace number 5

    bindsym $mod+Shift+6 move container to workspace A
    bindsym $mod+Shift+7 move container to workspace B
    bindsym $mod+Shift+8 move container to workspace C
    bindsym $mod+Shift+9 move container to workspace D
    bindsym $mod+Shift+0 move container to workspace E

    bindsym $mod+Ctrl+Shift+6 move container to workspace A
    bindsym $mod+Ctrl+Shift+7 move container to workspace B
    bindsym $mod+Ctrl+Shift+8 move container to workspace C
    bindsym $mod+Ctrl+Shift+9 move container to workspace D
    bindsym $mod+Ctrl+Shift+0 move container to workspace E
    # Note: workspaces can have any name you want, not just numbers.
    # We just use 1-10 as the default.

    # Default display outputs for workspaces
    workspace 1 output $disp2
    workspace 2 output $disp2
    workspace 3 output $disp2
    workspace 4 output $disp2
    workspace 5 output $disp2

    workspace A output $disp1
    workspace B output $disp1
    workspace C output $disp1
    workspace D output $disp1
    workspace E output $disp1
